<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:width="56dp"
        android:height="56dp">
        <shape android:shape="oval">
            <gradient
                android:startColor="#e63946"
                android:endColor="#4285f4"
                android:angle="45"
                android:type="linear" />
        </shape>
    </item>

    <item
        android:width="50dp"
        android:height="50dp"
        android:left="3dp"
        android:top="3dp"
        android:right="3dp"
        android:bottom="3dp">
        <shape android:shape="oval">
            <solid android:color="#66000000" />
        </shape>
    </item>

    <item
        android:width="46dp"
        android:height="46dp"
        android:left="5dp"
        android:top="5dp"
        android:right="5dp"
        android:bottom="5dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>

</layer-list>